{
  "env": {
    "jest": true
  },
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": ["error", "as-needed", { "requireForBlockBody": true }],
    "function-paren-newline": "off",
    "no-underscore-dangle": "off",
    "import/extensions": ["error", "ignorePackages"],
    "max-len": [
      "error",
      {
        "code": 120
      }
    ]
  }
}
