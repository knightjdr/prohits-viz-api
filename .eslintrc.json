{
  "env": {
    "es2021": true,
    "jest/globals": true,
    "node": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "jest"
  ],
  "root": true,
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "semi": [2, "always"]
  }
}
